{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": [
        "Example/app/main.ts",
        "Example/src/notMain.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}